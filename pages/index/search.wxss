/* pages/search/index.wxss */
.search-header {
  /* background: #9d9b62; */
  background: #00b2ee;
  background:-webkit-gradient(linear,top,bottom, color-stop(#66ccff), color-stop(#00B2EE));
  background:-webkit-linear-gradient(top, #66ccff, #00B2EE);
  background:linear-gradient(top, #66ccff, #00B2EE);
  padding: 40rpx 60rpx 40rpx 10rpx;
  font-size: 30rpx;
}
.row-box {
  height: 56rpx;
  line-height: 56rpx;
  margin-bottom: 20rpx;
}
.col-left {
  text-align: right;
  color: #ffffff;
  /* color: #79787A; */
}
.picker{
  padding:0 10rpx;
  color:#333333;
}
.select {
  background: #FFF;
  border-radius:5px;
  padding-left: 10rpx;
  padding-right: 10rpx;
  height:54rpx;
  /* border: 1px solid #000; */
}
/* .input{
  width: 8%;
  display: inline-block;
} */
.phones {
  display: flex;
  justify-content: space-between;
}
.phones input{
  width: 100%;
  /* text-align: center; */
}
.but {
  height: 56rpx;
  line-height: 56rpx;
  padding-left: 0;
  padding-right: 0;
  font-size: 28rpx;
  /* background: #ffcf02; */
  border-radius: 25rpx;
  border: 0;
  color: #fff;
  background-color:#00B2EE;
background:-webkit-gradient(linear,top,bottom, color-stop(#00B2EE), color-stop(#79d9f9));
background:-webkit-linear-gradient(top, #00B2EE, #79d9f9);
background:linear-gradient(top, #00B2EE, #79d9f9);
box-shadow:4rpx 2rpx 2rpx #0791bf;

}
.box{
  display: flex;
}
.box-1{
  flex-direction: row;
  flex-wrap: wrap;
  margin: 0 2%;
}
.box-item{
  width: 46%;
  text-align: left;
  margin: 2%;
  /* box-sizing:border-box; */
  padding: 3% 0;
  box-shadow: 6rpx 6rpx 22rpx #ddd;
  border-radius: 20rpx;
  font-size:24rpx;
}
.phone {
  font-weight:600;
  font-size: 32rpx;
  height: 34rpx;
  line-height: 34rpx;
  margin-bottom: 12rpx;
  text-align:center;
}
.padlf-68{
  padding-left:50rpx;
}
.color-cyan {
  color:#00b2ee;
}
.color-orange{
  color:red;
  font-weight: 700;
}
.refresh {
  font-size: 22rpx;
  text-align: right;
  margin: 20rpx 5%;
}
.exchange {
  display: inline-block;
  padding: 6rpx 10rpx;
  background-color: #00B2EE;
  background: -webkit-gradient(linear,top,bottom, color-stop(#00B2EE), color-stop(#79d9f9));
  background: -webkit-linear-gradient(top, #00B2EE, #79d9f9);
  background: linear-gradient(top, #00B2EE, #79d9f9);
  box-shadow: 4rpx 2rpx 2rpx #0791bf;
  color: #fff;
  text-align: center;
  border-radius: 10rpx;
}
.nomore {
  margin-top: 60rpx;
}
.nomore image {
  width: 100rpx;
}
.liang {
  display: inline-block;
  color: #fff;
  background-color: orange;
  font-size: 20rpx; 
  line-height: 32rpx;
  padding: 0 6rpx;
  border-radius: 10rpx;
  font-weight: normal;
  margin-right: 6rpx;
  vertical-align: super;
}

